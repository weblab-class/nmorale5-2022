{
  "compilerOptions": {
    "target": "es5",                        
    "module": "commonjs",                     
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "./server/@types", 
      "./node_modules/@types"
    ]
  }
}
